{
    "source": {
      "include": ["src"],
      "exclude": ["node_modules"],
      "includePattern": ".*\\.js$",
      "excludePattern": ".*\\.test\\.js$"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "template": "node_modules/minami"
    },
    "plugins": ["plugins/markdown"]
  }
